<?xml version="1.0" encoding="utf-8"?>

<!-- Place in frame layout to support child layout adding -->
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/signupPhoneFrameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.signup.phone.SignupPhoneActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <include
            layout="@layout/activity_signup_header"
            android:id="@+id/signupHeader"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            />
        <FrameLayout
            android:id="@+id/containerPhone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>
</FrameLayout>
